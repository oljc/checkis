{
  "name": "checkis",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "homepage": "https://checkis.cn",
  "bugs": {
    "url": "https://github.com/oljc/checkis/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oljc/checkis.git",
    "directory": "core"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./types": "./dist/types/index.d.ts",
    "./package.json": "./package.json"
  },
  "unpkg": "./dist/checkis.min.js",
  "browser": "./dist/checkis.min.js",
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "test": "rstest",
    "test:dev": "rstest watch",
    "check:types": "tsc --noEmit"
  },
  "devDependencies": {
    "@rslib/core": "^0.13.3",
    "@rstest/core": "^0.4.1"
  },
  "keywords": [
    "type-check",
    "validation",
    "is",
    "javascript",
    "typescript",
    "utils"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
